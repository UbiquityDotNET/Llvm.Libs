<?xml version="1.0" encoding="utf-8"?>
<!--
INPUT:
    $packageID$ - ID of the package
    $version$ - Version of this package (and all dependent packages)
    $authors$ - Authors of the pacakge
    $description$ - Description of the pacakge
    $tags$ - Tags to mark this package (Helps searching)
    $licExpression$ - License expression
    $projectUrl$ - URL for the project
    $runtimeId$ - Runtime ID this meta package is for
-->
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata minClientVersion="4.9.0">
        <id>$packageID$</id>
        <version>$version$</version>
        <authors>$authors$</authors>
        <description>$description$</description>
        <tags>$tags$</tags>
        <license type="expression">$licExpression$</license>
        <projectUrl>$projectUrl$</projectUrl>
        <dependencies>
            <!-- list each per-rid target package here -->
            <dependency id="LibLLVM-$runtimeId$-AArch64" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-AMDGPU" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-ARM" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-AVR" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-BPF" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-Hexagon" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-Lanai" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-LoongArch" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-Mips" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-MSP430" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-NVPTX" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-PowerPC" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-RISCV" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-Sparc" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-SPIRV" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-SystemZ" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-VE" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-WebAssembly" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-X86" version="$version$"/>
            <dependency id="LibLLVM-$runtimeId$-XCore" version="$version$"/>
        </dependencies>
    </metadata>
    <files/>
</package>
